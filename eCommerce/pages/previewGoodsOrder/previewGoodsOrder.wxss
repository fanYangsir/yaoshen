}
.price-count {
  color: #ff7100;
  font-size: 34rpx;
}
.bottom-nav form {
  float: right;
  width: 234rpx;
}
.bottom-nav button {
  float: right;
  width: 100%;
  font-size: 30rpx;
  border-radius: 0;
  color: #fff;
  background-color: #ff7100;
  line-height: inherit;
}
.discount-title {
  text-align: right;
  margin-top: 15rpx;
  color: #ff7100;
}
.selected-discount-title {
  display: inline-block;
  color: #666;
}
.preview-box .icon-rightarrow {
  font-size: 24rpx;
  color: #D2D2D2;
  margin-left: 10rpx;
}
.balance-price {
  margin-bottom: 20rpx;
  overflow: hidden;
  color: #ff7100;
}
switch {
  transform: scale(0.8);
}
.balance-switch {
  line-height: 80rpx;
}
.balance-switch switch {
