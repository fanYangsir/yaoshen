<wxs module="newCountModule">
  var pxToRpx = function (size) {
    return parseInt(size) * 2.34375;
  }
  var countImageSrc = function (allCount, todayCount, customFeature) {
    var counted = customFeature.type == 0 ? (customFeature.isSupportCancel == 0 && customFeature.effect == 1) ? (todayCount > 0) : (allCount > 0) : false;
    return counted ? customFeature.voteChangePicture : customFeature.voteInitPicture;
  }
  var getParentType = function (pcid) {
    if (!pcid) {
      return '';
    }
    var i = 0;
    while (pcid.charCodeAt(i) > 57 || pcid.charCodeAt(i) < 48) {  i++;  }
    return pcid.substring(0, i);
  }
  var isWantPanentType = function (pcid, target) {
    return getParentType(pcid) === target;
  }
  module.exports = {
    pxToRpx : pxToRpx,
    countImageSrc: countImageSrc,
    getParentType: getParentType,
    isWantPanentType: isWantPanentType
  }
</wxs>
