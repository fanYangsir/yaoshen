                  <button class="remain" catchtap='remainMe' data-goodsId="{{item.goods_id}}" data-activityId="{{item.activity_id}}"
                    data-index="{{index}}">提醒我</button>
                </block>
                <block wx:elif="{{item.status == 2}}">
                  <button class="reminded">已提醒</button>
                </block>
                <block wx:elif="{{item.status == 3}}">
                  <button class="go">{{item.activity_type == 4 ? '参与抽奖' : '去拼团' }}</button>
                </block>
                <block wx:elif="{{item.status == 4}}">
                  <button class="over">已结束</button>
                </block>
              </view>
            </view>
          </view>
        </view>
      </view>
    </block>

    <block wx:elif="{{data.customFeature.mode == 3}}">
      <view class="js-list-container group-list bigPicture">
        <view class="background-ele goods-item" wx:for="{{data.goods_data}}" wx:key="" data-goodsId="{{item.goods_id}}"
          data-activityId="{{item.activity_id}}" data-status="{{item.status}}" catchtap='gotoGroupDetail' style="{{'height:' + data.customFeature.lineHeight * (750/320) + 'rpx;margin-bottom:' + data.customFeature.margin*(750/320) + 'rpx;' + 'background:' + data.customFeature.lineBackgroundColor}}">
          <view class="image-wrap" style="{{'width:' + data.customFeature.imgWidth*(750/320)+'rpx;height:' + data.customFeature.imgHeight*(750/320)+'rpx;'}}">
            <image src='{{item.cover}}'></image>
            <block wx:if="{{item.status != 4 &&data.customFeature.isShowCountdown}}">
              <view class="count-down {{(item.status == 0 || item.status == 1) ? 'ready' : 'going'}}" wx:if="{{!(item.status == 3 && item.end_date == '-1') && item.status != 4}}">
                <view>
                  <block wx:if="{{item.status != 3}}">
                    距开始
                  </block>
                  <block wx:if="{{item.status == 3 && item.end_date != '-1'}}">
                    距结束
                  </block>
                </view>
                <view>
                  <view>{{item.downCount.hours}}</view>
                  <text> : </text>
                  <view style="width:48rpx;">{{item.downCount.minutes}}</view>
                  <text> : </text>
                  <view style="width:48rpx;">{{item.downCount.seconds}}</view>
                </view>
              </view>
            </block>
          </view>
          <view class="content-wrap">
            <view class="title">
              <view class="type" wx:if="{{data.customFeature.isShowGroupBuyStatus}}" style="{{'background:' + data.customFeature.lineBackgroundColor}}">
                <view>
                  <icon class="ico-moon icon-number-people"></icon>
                </view>
                <text>{{item.activity_type== 0 ? (item.num_of_people.length > 1 ? '多人团' : item.num_of_people[0] + '人团')
                  : (item.activity_type == 1 ? '新人团' : (item.activity_type == 2 ? '阶梯团' : (item.activity_type == 3 ?
                  '帮帮团' : '抽奖团')))}}</text>
              </view>{{item.title}}
            </view>
            <view class="sales-status">
              <view class="price">
                <text class="min-price">¥ <text style='font-size:36rpx;'>{{item.group_buy_price}}</text></text>
                <text class="origin-price" wx:if="{{data.customFeature.isShowVirtualPrice}}"> ¥ {{item.original_price}}</text>
                <view class="sales" wx:if="{{data.customFeature.isShowSales}}">已团{{item.goods_count}}件</view>
              </view>
              <view class="status">
                <view class="to-join">
                  <block wx:if="{{item.status == 0}}">
                    <button class="ready">即将开团</button>
                  </block>
                  <block wx:elif="{{item.status == 1}}">
                    <button class="remain" catchtap='remainMe' data-goodsId="{{item.goods_id}}" data-activityId="{{item.activity_id}}"
                      data-index="{{index}}">提醒我</button>
                  </block>
                  <block wx:elif="{{item.status == 2}}">
                    <button class="reminded">已提醒</button>
                  </block>
                  <block wx:elif="{{item.status == 3}}">
                    <button class="go">{{item.activity_type == 4 ? '参与抽奖' : '去拼团' }}</button>
                  </block>
                  <block wx:elif="{{item.status == 4}}">
                    <button class="over">已结束</button>
                  </block>
                </view>
              </view>
            </view>
          </view>
        </view>
      </view>
    </block>
    <template wx:if="{{data.loading}}" is="requestLoading"></template>
    <template wx:if="{{data.is_more == 0 && data.goods_data.length == 0}}" is="requestNone" data="{{data: '暂无商品'}}"></template>
    <template wx:if="{{data.loadingFail}}" is="requestFail" data="{{refreshEvent: 'catchMoreGroupList', compId: data.compId}}"></template>
    <template wx:if="{{data.is_more == 0 && data.goods_data.length > 0 && data.customFeature.isShowFinishText}}" is="requestAll"></template>
    <view class="loading-btn" wx:if="{{data.customFeature.vesselAutoheight == 1 && data.customFeature.loadingMethod == 1 && data.customFeature.mode != 2 && data.is_more && data.is_more != 0 && !data.loading && !data.loadingFail}}"
      data-compid="{{data.compId}}" bindtap="catchMoreGroupList">
      <image wx:if="{{data.customFeature.loadingStyle == 'img'}}" mode="aspectFill" src="{{data.customFeature.loadingImg}}"></image>
      <text wx:elif="{{data.customFeature.loadingStyle == 'text'}}" style="color: {{data.customFeature.loadingColor}};">{{data.customFeature.loadingText}}</text>
    </view>
  </scroll-view>
</template>

<template name="zcGroupStatus">

  <view style="{{data.style+';font-size:'+data.customFeature.fontSize+';color:'+data.customFeature.fontColor+';font-weight:'+data.customFeature.fontWeight+';font-style:'+data.customFeature.fontStyle+';text-decoration:'+data.customFeature.textDecoration+';'}}"
    class="group-status-list">
    <view class="list-status">
      <scroll-view scroll-x="true" style="{{'background:'+data.customFeature.background+';'}};width:100%;white-space:nowrap;text-decoration:inherit;overflow-x:scroll;overflow-y:hidden;" class="s-view">
        <view wx:if="{{data.customFeature.mode ==1}}" class="tabbar-list tabbar-list1" style="{{data.style+';font-size:'+data.customFeature.fontSize+';color:'+data.customFeature.fontColor+';font-weight:'+data.customFeature.fontWeight+';font-style:'+data.customFeature.fontStyle+';text-decoration:'+data.customFeature.textDecoration+';'}}"
          data-compid="{{data.compId}}">
          <view catchtap='catchGroupList' wx:for="{{data.customFeature.checkOptionsOne}}" wx:for-index="index" data-index="{{index}}" wx:if="{{item.checked}}" wx:key="" data-compid="{{data.compId}}" data-status="0" class="item" style="min-width:{{data.customFeature.checkedNum == 2 ? '375rpx' : (data.customFeature.checkedNum == 3 ? '250rpx' : '187rpx')}}">
            <view style="{{data.customFeature.selectNum == index ? 'color:'+data.customFeature.selectColor+';border-bottom:4rpx solid '+data.customFeature.selectColor+';font-style:'+data.customFeature.selectFontStyle+';text-decoration:'+data.customFeature.selectTextDecoration:'color:'+data.customFeature.defalutColor}}">{{item.label}}</view>
          </view>
        </view>
        <view wx:if="{{data.customFeature.mode ==2}}" class="tabbar-list tabbar-list2" style="{{data.style+';font-size:'+data.customFeature.fontSize+';color:'+data.customFeature.fontColor+';font-weight:'+data.customFeature.fontWeight+';font-style:'+data.customFeature.fontStyle+';text-decoration:'+data.customFeature.textDecoration+';'}}">

          <view catchtap='catchGroupList' wx:for="{{data.customFeature.checkOptionsOne}}" wx:for-index="index" data-index="{{index}}" wx:if="{{item.checked}}" wx:key="" data-compid="{{data.compId}}" data-status="0" class="item" style="min-width:{{data.customFeature.checkedNum == 2 ? '375rpx' : (data.customFeature.checkedNum == 3 ? '250rpx' : '187rpx')}}">
            <view style="{{data.customFeature.selectNum == index ? 'background:'+data.customFeature.selectColor+';color:#fff;font-style:'+data.customFeature.selectFontStyle+';text-decoration:'+data.customFeature.selectTextDecoration:'color:'+data.customFeature.defalutColor}}">{{item.label}}</view>
          </view>
        </view>
      </scroll-view>
    </view>
  </view>

</template>